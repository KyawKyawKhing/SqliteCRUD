<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.nw.kcube.sqlitdbcrud">

    <application
        android:name="com.nw.kcube.sqlitdbcrud.other.app"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name="com.nw.kcube.sqlitdbcrud.MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name="com.nw.kcube.sqlitdbcrud.AddMovieActivity"
            android:parentActivityName="com.nw.kcube.sqlitdbcrud.MainActivity" />
        <activity
            android:name="com.nw.kcube.sqlitdbcrud.UpdateMovieActivity"
            android:parentActivityName="com.nw.kcube.sqlitdbcrud.DetailMovieActivity" />
        <activity
            android:name="com.nw.kcube.sqlitdbcrud.DetailMovieActivity"
            android:parentActivityName="com.nw.kcube.sqlitdbcrud.MainActivity" />
    </application>

</manifest>